<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CMPUT391_RIS</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>UploadImageLogicSQL</servlet-name>
    <servlet-class>org.ris.servlets.UploadImageLogicSQL</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PictureBrowse</servlet-name>
    <servlet-class>org.ris.servlets.PictureBrowse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetOnePic</servlet-name>
    <servlet-class>org.ris.servlets.GetOnePic</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadImageLogicSQL</servlet-name>
    <url-pattern>/UploadImage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PictureBrowse</servlet-name>
    <url-pattern>/PictureBrowse</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetOnePic</servlet-name>
    <url-pattern>/GetOnePic</url-pattern>
  </servlet-mapping>
</web-app>